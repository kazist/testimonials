    "query": {
        "from": {
            "table": "Testimonials\\Testimonials\\Code\\Tables\\Testimonials",
            "alias": "tt",
            "name": "#__testimonials_testimonials"
        },
        "select": [
            "tt.id",
            "tt.user_id",
            "uu.name AS user_id_name",
            "tt.title",
            "tt.testimony",
            "tt.image",
            "mm.title AS image_title",
            "mm.file AS image_file",
            "tt.user_title",
            "tt.name",
            "tt.company",
            "tt.published",
            "tt.featured",
            "tt.created_by",
            "uu_9392.name AS created_by_name",
            "tt.date_created",
            "tt.modified_by",
            "uu_6062.name AS modified_by_name",
            "tt.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "tt.user_id = uu.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "tt.image = mm.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_9392",
                "name": "#__users_users",
                "on": "tt.created_by = uu_9392.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_6062",
                "name": "#__users_users",
                "on": "tt.modified_by = uu_6062.id"
            }
        ]
    }
